<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/r/remote"
            factory-class="org.knime.ext.r.node.RPlotterNodeFactory"
            id="org.knime.ext.r.node.RPlotterNodeFactory"/>
      <node
            category-path="/r/remote"
            factory-class="org.knime.ext.r.node.RConsoleFactory"
            id="org.knime.ext.r.node.RConsoleFactory"/>
      <node
            category-path="/r/local"
            factory-class="org.knime.ext.r.node.local.RLocalScriptingNodeFactory"
            id="org.knime.ext.r.node.local.RLocalScriptingNodeFactory">
      </node>
      <node
            category-path="/r/local"
            factory-class="org.knime.ext.r.node.local.RLocalViewsNodeFactory"
            id="org.knime.ext.r.node.local.RLocalViewsNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/r.png"
            level-id="r"
            name="R"
            path="/"/>
      <category
            icon="icons/missing.gif"
            level-id="local"
            name="Local"
            path="/r">
      </category>
      <category
            icon="icons/missing.gif"
            level-id="remote"
            name="Remote"
            path="/r">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.knime.workbench.ui.headless"
            class="org.knime.ext.r.preferences.RPreferencePage"
            id="org.knime.ext.r.preferences.RPreferencePage"
            name="R">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.knime.ext.r.preferences.RPreferenceInitializer">
      </initializer>
   </extension>
</plugin>
