_S_e_e _A_l_s_o:

     'pmml', 'kmeans'.

_E_x_a_m_p_l_e_s:

     ds <- rbind(matrix(rnorm(100, sd = 0.3), ncol = 2),
                     matrix(rnorm(100, mean = 1, sd = 0.3), ncol = 2))
     colnames(ds) <- c("Dimension1", "Dimension2")
     cl <- kmeans(ds, 2)
     pmml(cl)

